generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id               String            @id @default(uuid())
  email            String?
  avatar_url       String            @default("temp avatar_url")
  created_at       DateTime          @default(now())
  display_name     String?           @default("temp display_name")
 }
